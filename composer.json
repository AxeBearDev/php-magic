{
  "name": "axebear/php-magic",
  "description": "PHP micro framework for hooking into magic methods using docblocks and attributes.",
  "type": "library",
  "license": "MIT",
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "AxeBear\\Magic\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Alex Barrett",
      "email": "alex@axebear.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "phpstan/phpdoc-parser": "^1.28"
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "psy/psysh": "@stable"
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "cli": "vendor/bin/psysh"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
